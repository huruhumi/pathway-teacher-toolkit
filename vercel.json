{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/planner/(.*)",
      "destination": "/planner/index.html"
    },
    {
      "source": "/essay-lab/(.*)",
      "destination": "/essay-lab/index.html"
    },
    {
      "source": "/nature-compass/(.*)",
      "destination": "/nature-compass/index.html"
    },
    {
      "source": "/academy-ops/(.*)",
      "destination": "/academy-ops/index.html"
    }
  ]
}